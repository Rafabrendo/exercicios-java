package application;

import java.util.Map;
import java.util.TreeMap;

public class Program {

	public static void main(String[] args) {
		
		Map<String, String> cookies = new TreeMap<>();
		//O tipo da key do Map é String e o value é String tbm
		//Uma estrutura para armazenar cookies.
		
		cookies.put("username", "Maria");
		cookies.put("email", "maria@gmail.com");
		cookies.put("phone", "99711122");
		
		cookies.remove("email");
		//Estou removendo do meu map aquela chave(key) cujo apelido é email
		
		cookies.put("phone", "99733322");
		//nessa inserção, como já tenho a chave phone, vai ser sobreposto esse novo valor nessa chave
		
		System.out.println("ALL COOKIES: ");
		for(String key: cookies.keySet()) {
			//a função keySet() vai retornar as chaves do map
			System.out.println(key + ": " + cookies.get(key));
		}
		/*ALL COOKIES: 
		phone: 99711122
		username: Maria*/
		
		//removi o email e por isso não apareceu
		//além disso apareceu tudo em ordem alfabetica, por causa do TreeMap.
		
		
		
	}

}
